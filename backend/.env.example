# LLM Provider Configuration
# Options: gemini, cerebras, openai
LLM_PROVIDER=cerebras

# Gemini Configuration
GEMINI_API_KEY=your_gemini_api_key
GOOGLE_API_KEY=your_google_api_key
GEMINI_MODEL=gemini-2.0-flash-lite

# Cerebras Configuration
CEREBRAS_API_KEY=your_cerebras_api_key
CEREBRAS_MODEL=llama3.1-8b

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4o-mini

# LiveKit Configuration
# The LIVEKIT_URL is provided to the frontend via the /api/token endpoint
LIVEKIT_URL=wss://your-livekit-server.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# Deepgram Configuration
DEEPGRAM_API_KEY=your_deepgram_api_key

# Beyond Presence Configuration
BEY_API_KEY=your_bey_api_key
BEY_AVATAR_ID=your_bey_avatar_id

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017
MONGODB_DB_NAME=voice_agent

# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO
MAX_CONCURRENT_CALLS=10
